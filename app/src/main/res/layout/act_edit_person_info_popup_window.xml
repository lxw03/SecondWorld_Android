<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="156px"
    android:background="@color/white"
    tools:context=".modular.prepareModule.improvePersonInfo.view.ImprovePersonInfoAct">

    <TextView
        android:id="@+id/tv_act_edit_person_info_popup_window_image"
        android:layout_width="match_parent"
        android:layout_height="78px"
        android:gravity="center"
        android:text="相册"
        android:textColor="@color/common_app_color" />

    <View
        android:layout_width="match_parent"
        android:layout_centerInParent="true"
        android:layout_height="1px"
        android:background="@color/gray_two" />

    <TextView
        android:id="@+id/tv_act_edit_person_info_popup_window_cancel"
        android:layout_width="match_parent"
        android:layout_height="78px"
        android:layout_below="@+id/tv_act_edit_person_info_popup_window_image"
        android:gravity="center"
        android:text="取消"
        android:textColor="@color/common_app_color" />

</RelativeLayout>
